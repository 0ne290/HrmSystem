@model SalaryViewModel

@{
    ViewBag.Title = "Employee salary";
    Layout = "_Layout";
}

@section Links {
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/meyer-reset/2.0/reset.min.css">
          <link rel='stylesheet prefetch' href='http://maxcdn.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css'>
	<link rel="stylesheet" href="/css/style.css">
}

<table class="table">
	<thead>
	<tr>
		<th>Эффективность</th>
		<th>Премиальная ставка</th>
		<th>Премия с учетом эффективности</th>
		<th>Зарплатная ставка</th>
		<th>Зарплата с учетом премии</th>
	</tr>
	</thead>
	<tbody>
	<tr>
		<td>@Model.Efficiency</td>
		<td>@Model.PremiumRate</td>
		<td>@Model.Premium</td>
		<td>@Model.SalaryRate</td>
		<td>@Model.Salary</td>
	</tr>
	</tbody>
</table>
<script src='http://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js'></script>